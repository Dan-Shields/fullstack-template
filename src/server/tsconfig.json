{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"target": "esnext",
		"module": "CommonJS",
		"moduleResolution": "node",
		"strict": true,
		"sourceMap": true,
		"noEmit": false,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"lib": ["ESNext"],
		"outDir": "../../build/server"
	},
	"exclude": [
		"../client"
	]
}
